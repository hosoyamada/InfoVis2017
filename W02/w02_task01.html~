  </head>
  <body>
    <script>
      Vec3 = function(x,y,z){
      this.max = x;
      if(this.max < y){
      this.max = y;
		    }
		    
      if(this.max < z){
      this.max = z;
		    }
      this.min = x;
      if(this.min < y){
      this.min = y;
      }
      if(this.min < z){
      this.min = z;
		    }
		    this.mid = (x+y+z)-(this.max+this.min);
		    }
		    var x=5,y=4,z=8;
		    var v=new Vec3(x,y,z);
		    var min = v.min();
		    var mid = v.mid();
		    var max = v.max();
		    
      document.write(x);
      document.write(y);
      document.write(z);
    </script>
  </body>
